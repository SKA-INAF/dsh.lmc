	<Attributes>

		<!-- ================================================== -->
		<!-- ==      State & Modes                           == -->
		<!-- ================================================== -->
		
		<Attribute name = "operatingMode" alias="operatingMode">
			<Description> DS sub-element mode </Description>
			<DataType> ENUM </DataType>
			<DataFormat> SCALAR </DataFormat> 
			<RWType> READ_WRITE </RWType>	
			<PossibleValues>
				<Value> OFF </Value>
				<Value> STARTUP </Value>
				<Value> STANDBY-LP </Value>
				<Value> STOW </Value> 
				<Value> ESTOP </Value> 
				<Value> POINT </Value>
				<Value> MAINTENANCE </Value>
				<Value> UNKNOWN </Value> <!-- Not present in ICD -->
				<Value> STANDBY-FP </Value> <!-- Not present in ICD -->
			</PossibleValues>
			<Polling> 
				<Period> {{ds_fast_poll_period}} </Period>
			</Polling>
			<Events>
				<PushEvent> </PushEvent>
				<AbsChangeEvent>
					<ChangeValue> 1 </ChangeValue>
				</AbsChangeEvent>
				<PeriodicEvent>
					<Period> {{ds_default_periodic_event_period}} </Period>
				</PeriodicEvent>
				<ArchiveEvent>
					<Period> {{ds_very_slow_archive_event_period}} </Period>
					<AbsChange> 1 </AbsChange>
				</ArchiveEvent>			
			</Events>
			<Value>
				<InitValue> UNKNOWN </InitValue>
			</Value>
		</Attribute>

		<Attribute name = "powerState" alias="powerState">
			<Description> Current sub-element power state </Description>
			<DataType> ENUM </DataType>
			<DataFormat> SCALAR </DataFormat>
			<RWType> READ_WRITE </RWType>	
			<PossibleValues>
				<Value> OFF </Value>
				<Value> UPS </Value>
				<Value> FULL-POWER </Value>
				<Value> LOW-POWER </Value>
			</PossibleValues> 
			<Polling> 
				<Period> {{ds_fast_poll_period}} </Period>
			</Polling>
			<Events>
				<PushEvent> </PushEvent>
				<AbsChangeEvent>
					<ChangeValue> 1 </ChangeValue>
				</AbsChangeEvent>
				<PeriodicEvent>
					<Period> {{ds_default_periodic_event_period}} </Period>
				</PeriodicEvent>
				<ArchiveEvent>
					<Period> {{ds_very_slow_archive_event_period}} </Period>
					<AbsChange> 1 </AbsChange>
				</ArchiveEvent>	
			</Events>
			<Value>
				<InitValue> OFF </InitValue>
			</Value>
		</Attribute>


		<Attribute name = "indexerPosition" alias="indexerPosition">
			<Description> Current indexer position </Description>
			<DataType> ENUM </DataType>
			<DataFormat> SCALAR </DataFormat>
			<RWType> READ_WRITE </RWType>	
			<Memorized>  </Memorized>  <!-- Memorize previous band -->
			<PossibleValues>
				<Value> UNKNOWN </Value> 
				<Value> B1 </Value>
				<Value> B2 </Value>
				<Value> B3 </Value>
				<Value> B4 </Value>
				<Value> B5 </Value>
				<Value> MOVING </Value> <!-- Added wrt ICD -->
				<Value> ERROR </Value> <!-- Added wrt ICD -->
				<!-- <Value> UNKNOWN </Value> --> <!-- Added wrt ICD (set in LMC?) -->
			</PossibleValues> 
			<Polling> 
				<Period> {{ds_fast_poll_period}} </Period>
			</Polling>
			<Events>
				<PushEvent> </PushEvent>
				<AbsChangeEvent>
					<ChangeValue> 1 </ChangeValue>
				</AbsChangeEvent>
				<PeriodicEvent>
					<Period> {{ds_very_slow_periodic_event_period}} </Period>
				</PeriodicEvent>
				<ArchiveEvent>
					<Period> {{ds_very_slow_archive_event_period}} </Period>
					<AbsChange> 1 </AbsChange>
				</ArchiveEvent>	
			</Events>	
			<Value>
				<InitValue> B1 </InitValue>
			</Value>
		</Attribute>
		
		<Attribute name = "interfaceType" alias="interfaceType">	
			<Description> Type of interface to be managed </Description>
			<DataType> ENUM </DataType>
			<DataFormat> SCALAR </DataFormat> 
			<RWType> READ_WRITE </RWType>	
			<PossibleValues>
