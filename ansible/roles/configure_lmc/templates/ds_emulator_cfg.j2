				<ArchiveEvent>
					<Period> {{ds_very_very_slow_archive_event_period}} </Period>
					<AbsChange> 1 </AbsChange>
				</ArchiveEvent>
			</Events>
			<Value>
				<InitValue> EMULATED </InitValue>
			</Value>
		</Attribute>
	
