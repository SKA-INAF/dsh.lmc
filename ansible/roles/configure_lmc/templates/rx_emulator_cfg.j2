		<!-- D.I.M.LMC_SRx.D005: Only STANDBY-LP/STANDBY-FP/MAINTENANCE/DATA-CAPTURE/UNKNOWN can be set by LMC -->
		<Attribute name = "operatingMode" alias="operatingMode">
			<Description> Current sub-element mode </Description>
			<DataType> ENUM </DataType>
			<DataFormat> SCALAR </DataFormat> <!-- SCALAR/SPECTRUM/IMAGE -->
			<RWType> READ_WRITE </RWType>	<!-- READ/WRITE/READ_WRITE -->
			<PossibleValues>
				<Value> UNKNOWN </Value> <!-- Set by LMC when connection is not established or when it is lost -->
				<Value> OFF </Value>
				<Value> STARTUP </Value>
				<Value> STANDBY-LP </Value>
				<Value> STANDBY-FP </Value>
				<Value> DATA-CAPTURE </Value>
				<Value> CONFIGURE </Value>
				<Value> MAINTENANCE </Value>
			</PossibleValues>	
			<Polling> 
				<Period> {{rx_fast_poll_period}} </Period>
			</Polling>
			<Events>
				<PushEvent> </PushEvent>
				<RelChangeEvent>
					<ChangeValue> {{default_attr_rel_change}} </ChangeValue>
				</RelChangeEvent>
				<AbsChangeEvent>
					<ChangeValue> 1 </ChangeValue>
				</AbsChangeEvent>
				<PeriodicEvent>
					<Period> {{rx_default_periodic_event_period}} </Period>
				</PeriodicEvent>
				<ArchiveEvent>
					<Period> {{rx_very_slow_archive_event_period}} </Period>
					<AbsChange> 1 </AbsChange>
				</ArchiveEvent>
			</Events>
			<Value>
				<InitValue> OFF </InitValue>
			</Value>
		</Attribute>

