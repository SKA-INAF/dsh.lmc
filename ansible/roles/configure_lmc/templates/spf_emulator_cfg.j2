		<!-- D.I.M.LMC_SPF.D005: Only STANDBY-LP/OPERATE/MAINTENANCE/UNKNOWN can be set by LMC -->
		<Attribute name = "operatingMode" alias="operatingMode">
			<Description> Current sub-element mode </Description>
			<DataType> ENUM </DataType>
			<DataFormat> SCALAR </DataFormat> 
			<RWType> READ_WRITE </RWType>	
			<PossibleValues>
				<Value> UNKNOWN </Value> <!-- Set by LMC when connection is not established or when it is lost -->
				<Value> OFF </Value>
				<Value> STARTUP </Value>
				<Value> STANDBY-LP </Value>
				<Value> OPERATE </Value>
				<Value> MAINTENANCE </Value>
				<Value> ERROR </Value>				
			</PossibleValues>
			<Polling> 
				<Period> {{spf_fast_poll_period}} </Period>
			</Polling>
			<Events>
				<PushEvent> </PushEvent>
				<RelChangeEvent>
					<ChangeValue> {{default_attr_rel_change}} </ChangeValue>
				</RelChangeEvent>
				<AbsChangeEvent>
					<ChangeValue> 1 </ChangeValue>
				</AbsChangeEvent>
				<PeriodicEvent>
					<Period> {{spf_default_periodic_event_period}} </Period>
				</PeriodicEvent>
				<ArchiveEvent>
					<Period> {{spf_very_slow_archive_event_period}} </Period>
					<AbsChange> 1 </AbsChange>
				</ArchiveEvent>
			</Events>
			<Value>
				<InitValue> OFF </InitValue>
			</Value>
		</Attribute>

