      </argout>
      <status abstract="false" inherited="false" concrete="true" concreteHere="true"/>
    </commands>
    <commands name="SetStowMode" description="This command triggers the Dish to transition to the &#xA;STOW Dish Element Mode, and returns to the caller. &#xA;To point the dish in a direction that minimises the &#xA;wind loads on the structure, for survival in strong wind &#xA;conditions. The Dish is able to observe in the stow &#xA;position, for the purpose of transient detection." execMethod="set_stow_mode" displayLevel="OPERATOR" polledPeriod="0" isDynamic="false">
